CREATE DATABASE SAMSONOVTAR;
use SAMSONOVTAR;

--Tabel loomine 
CREATE TABLE tootaja(
tootajaID int PRIMARY KEY identity (1,1),
eesnimi varchar(30),
perenimi varchar(30),
synniaeg date,
koormus decimal(5,2),
elukoht TEXT,
abielus bit
);
select * from tootaja;

